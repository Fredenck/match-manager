{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json","**/.*","**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
          "destination": "/matches/[slug]/index.html",
          "regex": "^/user/([^/]+?)(?:/)?$"
      }],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }
}